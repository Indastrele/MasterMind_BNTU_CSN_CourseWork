<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:vm="using:CourseWorkOnCSharp.ViewModels"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewModels="clr-namespace:CourseWorkOnCSharp.ViewModels"
        mc:Ignorable="d" d:DesignWidth="340" d:DesignHeight="150"
        Width="340" Height="150"
        x:Class="CourseWorkOnCSharp.Views.ResultWindow"
        x:DataType="vm:ResultWindowViewModel"
        Closing="ClosingWindow"
        Title="ResultWindow">
    <Design.DataContext>
        <viewModels:ResultWindowViewModel />
    </Design.DataContext>
    
    <TextBlock 
        Text="{Binding Result}" 
        FontWeight="UltraBold" 
        FontSize="24" 
        HorizontalAlignment="Center" 
        VerticalAlignment="Center"/>
</Window>
